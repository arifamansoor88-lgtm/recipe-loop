{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node"],

    "outDir": "dist"
  },
  "include": ["src/**/*", "prisma.config.ts"],
  "exclude": ["node_modules"]
}
